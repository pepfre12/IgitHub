local function BEOV_fake_script() -- Main.setup 
	local script = Instance.new('LocalScript', Main)

	for i,v in pairs(script.Parent:GetDescendants()) do
		if v:IsA("TextButton") then
			v.MouseButton1Click:connect(function()
				script.bruhhh:Play()
			end)
		end
	end
end
coroutine.wrap(BEOV_fake_script)()
local function ATNJU_fake_script() -- OpenClose.LocalScript 
	local script = Instance.new('LocalScript', OpenClose)

	local script = Instance.new('LocalScript', OpenClose)
	
	OpenClose.MouseEnter:Connect(function()
		local TweenService = game:GetService"TweenService"
		local tween = TweenService:Create(
			OpenClose,
			TweenInfo.new(1),
			{
				Rotation = 360
			}
		)
		tween:Play()
	end)
	OpenClose.MouseLeave:Connect(function()
		local TweenService = game:GetService"TweenService"
		local tween = TweenService:Create(
			OpenClose,
			TweenInfo.new(1),
			{
				Rotation = 0
			}
		)
		tween:Play()
	end)
	script.Parent.MouseButton1Click:Connect(function()
		Main.Visible = not Main.Visible
	end)
	end
end
coroutine.wrap(ATNJU_fake_script)()
